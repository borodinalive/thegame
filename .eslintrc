{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "require-jsdoc": 0,
    "max-len": [
      0,
      {
        "code": 120
      }
    ],
    "space-before-function-paren": 0
  }
}
